	pod 'Firebase/Storage'
	pod 'Plaid'
  target 'AllTheWageTests' do
    inherit! :search_paths
    # Pods for testing
  end
   
  target 'AllTheWageUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
